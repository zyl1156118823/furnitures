<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mook.house.house.Mapper.UserDao">
    <select id="findUserByStuidAndPassword" resultType="com.mook.house.house.entity.UserEntity">
        select * from user where name=#{username} and passwd=#{password}
    </select>
    <select id="findAllStu" resultType="com.mook.house.house.entity.UserEntity">
        select * from user
    </select>
    <insert id="addUser">
    </insert>
</mapper>